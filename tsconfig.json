{
  "compilerOptions": {
    "importHelpers": true,
    "module": "esnext",
    "esModuleInterop": true,
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "bundler",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "ES2022",
    "lib": [
      "es2022",
      "dom"
    ],
    "useDefineForClassFields": false
  }
}
